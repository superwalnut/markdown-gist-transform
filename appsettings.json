{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Github": {
    "GistApi": "https://api.github.com/gists"
  },
  "GistEmbed": {
    "GistBaseUrl": "https://gist.github.com",
    "EmbedTemplate": {
      "Dev.to": "{{% gist https://gist.github.com/{0}/{1} %}}",
      "Hugo": "{{{{< gist {0} {1} >}}}}",
      "Medium": "https://gist.github.com/{0}/{1}"
    }
  },
  "DevTo": {
    "ArticleApi": "https://dev.to/api/articles"
  },
  "Medium": {
    "ProfileApi": "https://api.medium.com/v1/me",
    "ArticleApi": "https://api.medium.com/v1/users/{0}/posts"
  }
}
